{
  "name": "rename-prop-mongo",
  "version": "0.0.12",
  "description": "Rename a property in a MongoDB collection",
  "main": "./build/bundle/main.js",
  "bin": {
    "rename-prop-mongo": "build/bundle/main.js"
  },
  "scripts": {
    "test": "echo \"crea test pls\"",
    "dev": "nodemon ./src/main.ts",
    "lint": "ts-standard",
    "tsc": "tsc",
    "start": "node build/build/main.js",
    "build": "tsc --build tsconfig.json",
    "build:watch": "tsc --build tsconfig.json --watch",
    "build:bundle": "tsc --build tsconfig.json && esbuild ./build/build/main.js --bundle --outfile=./build/bundle/main.js --minify --allow-overwrite --platform=node --target=node16",
    "clean": "make clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisRol2/renamePropMongo.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "esbuild",
    "mongodb"
  ],
  "author": "ChrisRol",
  "license": "CC-BY-ND-4.0",
  "bugs": {
    "url": "https://github.com/chrisRol2/renamePropMongo/issues"
  },
  "homepage": "https://github.com/chrisRol2/renamePropMongo",
  "dependencies": {
    "dotenv": "^16.0.1",
    "mongodb": "6.8.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/node": "17.0.35",
    "esbuild": "^0.15.7",
    "nodemon": "2.0.16",
    "ts-node": "10.8.0",
    "ts-node-dev": "1.1.8",
    "ts-standard": "11.0.0",
    "typescript": "4.7.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "./node_modules/ts-standard/config/ts-standard.json"
    ]
  }
}
